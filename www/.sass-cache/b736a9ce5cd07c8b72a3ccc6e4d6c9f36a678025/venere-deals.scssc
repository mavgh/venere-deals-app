box-shadow: none;
color: transparent;
mask-image: radial-gradient(center, ellipse cover, rgba(0,0,0,0) 0%,rgba(0,0,0,0) 55%,rgba(0,0,0,1) 56%,rgba(0,0,0,1) 100%);
-webkit-mask-image: -webkit-radial-gradient(center, ellipse cover, rgba(0,0,0,0) 0%,rgba(0,0,0,0) 55%,rgba(0,0,0,1) 56%,rgba(0,0,0,1) 100%);
position: fixed;
height: 94px;
