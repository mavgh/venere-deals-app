/* #000B -------------------- css3 animation keyframes -------------------- */
@keyframes spin {
0%   { transform:rotate(0deg); }
100% { transform:rotate(360deg); }
}
@-webkit-keyframes spin {
0% { -webkit-transform:rotate(0deg); }
100% { -webkit-transform:rotate(360deg); }
}

/*Hotel Detail Page*/
.hdp-hotel-info {
   padding: 0 13px;
}
#info-price {
   position: relative;
}
.hdp-hotel-name {
   margin: 6px 0;
}
.hdp-hotel-name .name {
   font-size: 22px;
   margin : 0 8px 0 0;
}

#stay-parameters {
   background-color: #CCC;
   font-size: 12px;
   padding: 4px;
}
#dates,
#rooms-number {
   font-weight: 700;
}

#dates,
#guest-rooms {
   display: inline;
}
#dates {
   margin: 0 13px 0 0;
}
#dates:after {
   background-color: #FFF;
   content: '';
   display: block;
   height: 5px;
   width: 8px;
   position: absolute;
   margin: -9px 0 0 4px;
}


#overview {
   margin: 0 13px;
}


#gallery {
   height: 212px;
}
#gallery ul {
   margin: 0;
   list-style: none;
   padding: 0;
}
#gallery ul,
#gallery ul li {
   height: 100%;
   width: 100%;   
}
#gallery ul li {
   background-size: cover;
}


/*DEAL CARDS LANDSCAPE*/
$squaredCard : 135px;
$arrowWidth:5px;
@media screen 
and (orientation : landscape) 
{
   .list li,
   .topcoat-list__item {
      height: $squaredCard;
      width: $squaredCard;
      margin: 13px 16px;
      float: left;
      clear: none;
   }
   .topcoat-list__item:before {
      content: none;
   }
   .list li:last-child,
   .topcoat-list__item:last-child {
      margin: 13px 16px;
   }
   
   .card-pic {
      border-top: none;
      height: 100%;
   }
   .card-title {
       background-color: rgba(255,255,255, 0.70);
       bottom: 0;
       left: 0;
       font-size: 22px;
       line-height: 22px;
       padding: 2px 6px 28px 4px !important;
       position: absolute;
       right: 0;
       width: auto;
       z-index: 1;
   }

   
   .card-subtitle,
   .cities .card-subtitle {
      bottom: ( ($arrowWidth / 2) + 4px);
      height: 10px;
      margin: 0 0 0 (-$arrowWidth);
      opacity: 1;
      padding: 4px 0 8px 11px;
      box-shadow: 9px 3px 3px 0px rgba(0, 0, 0, 0.26);
      
   }
   .card-subtitle:before {
       background-color: #000000;
       bottom: -($arrowWidth / 2);
       content: "";
       height: 0;
       left: -19px;
       position: absolute;
       width: 0;
   }
      
   .card-subtitle:before {
      background-color: transparent;
      border-style: solid;
      border-width: 0 $arrowWidth ($arrowWidth / 2) 0;
      border-color: transparent #000000 transparent transparent;
      border-style: inset;
      width: 0px;
      height: 0px;
      -webkit-transform:rotate(360deg);
   }
   
   .card-subtitle:after {
      text-shadow: 4px 2px 3px rgba(0, 0, 0, 0.0);
      top: 1px;
      -webkit-background-clip: text !important;
      -webkit-text-fill-color: transparent;      
   }
   
   
   .card-subtitle .landscape {
      font-size: 11px;
      font-weight: 400;
      padding: 0 21px 0 0;
      text-transform: uppercase;
   }
   
   $smallerLabel : 22px;
   .card-subtitle:after {
      height: $smallerLabel;
      font-size: $smallerLabel;
      right: -(($iconWidth / 2) + 5px);
   }
   .card-subtitle .description,
   .cities .card-subtitle .city-name,
   .cities .card-subtitle .city-price {
      display: none;
   }
   .card-subtitle .landscape {
      display: block;
   }
   .cities .card-title {
      display: block;
   }
   .cities .landscape-price {
      clear: both;
      display: block;
      font-size: 13px;
      font-style: italic;
   }
   .cities .landscape-price b {
      font-size: 17px;
      font-style: normal;
      font-weight: 400;
      padding: 0 0 0 6px;
   }

}

   @media screen and (-webkit-min-device-pixel-ratio:0)
   and (orientation : landscape)  
   {
      .card-subtitle:after {
         top:0;
      }
