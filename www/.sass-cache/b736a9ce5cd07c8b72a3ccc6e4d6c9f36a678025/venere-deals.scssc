filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#ffe1bf',GradientType=1 );
/*Android 4.2.2 stockbrowser doesn't support border-radiua shorthand, all four corners must be declared separately*/
border-top-left-radius: 100px;
border-top-right-radius: 100px;
border-bottom-left-radius: 0;
border-bottom-right-radius: 0;
content: "";
display: block;
height: 50%;
width: 100%;
}
.loading-content:after {
background: -moz-radial-gradient(center, ellipse cover,  rgba(233,233,233,1) 0%, rgba(233,233,233,1) 55%, rgba(233,233,233,0) 56%, rgba(233,233,233,0) 100%); /* FF3.6+ */
background: -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop(0%,rgba(233,233,233,1)), color-stop(55%,rgba(233,233,233,1)), color-stop(56%,rgba(233,233,233,0)), color-stop(100%,rgba(233,233,233,0))); /* Chrome,Safari4+ */
