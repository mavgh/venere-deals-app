opacity: 0.89;
visibility: visible;
animation:spin .75s infinite linear;
-webkit-animation:spin .75s infinite linear;
z-index: 999;
}
.loading-content:before {
background: rgb(233,233,233);
background: -moz-linear-gradient(left,  rgba(233,233,233,1) 0%, rgba(255,225,191,1) 86%, rgba(255,225,191,1) 100%);
background: -webkit-gradient(linear, left top, right top, color-stop(0%,rgba(233,233,233,1)), color-stop(86%,rgba(255,225,191,1)), color-stop(100%,rgba(255,225,191,1)));
