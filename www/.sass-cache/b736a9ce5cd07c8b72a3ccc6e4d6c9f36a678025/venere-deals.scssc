background: -o-radial-gradient(center, ellipse cover,  rgba(233,233,233,1) 0%,rgba(233,233,233,1) 55%,rgba(233,233,233,0) 56%,rgba(233,233,233,0) 100%); /* Opera 12+ */
background: -ms-radial-gradient(center, ellipse cover,  rgba(233,233,233,1) 0%,rgba(233,233,233,1) 55%,rgba(233,233,233,0) 56%,rgba(233,233,233,0) 100%); /* IE10+ */
