/*DEAL CARDS LANDSCAPE*/
$squaredCard : 135px;
$arrowWidth:5px;
@media screen 
and (orientation : landscape) 
{
   .list li {
      height: $squaredCard;
      width: $squaredCard;
      margin: 13px 0 0 13px;
      float: left;
      clear: none;
   }
   
   .list li:last-child {
      margin: 13px 16px;
   }
   
   .card-pic {
      border-top: none;
      height: 100%;
   }
   .card-title {
       background-color: rgba(255,255,255, 0.70);
       bottom: 0;
       left: 0;
       font-size: 22px;
       line-height: 22px;
       padding: 2px 6px 28px 4px !important;
       position: absolute;
       right: 0;
       width: auto;
       z-index: 1;
   }

   
   .card-subtitle,
   .cities .card-subtitle {
      bottom: ( ($arrowWidth / 2) + 4px);
      height: 10px;
      margin: 0 0 0 (-$arrowWidth);
      opacity: 1;
      padding: 4px 0 8px 11px;
      box-shadow: 9px 3px 3px 0px rgba(0, 0, 0, 0.26);
      
   }
   .card-subtitle:before {
       background-color: #000000;
       bottom: -($arrowWidth / 3);
       content: "";
       height: 0;
       left: -19px;
       position: absolute;
       width: 0;
   }
      
   .card-subtitle:before {
      background-color: transparent;
      border-style: solid;
      border-width: 0 $arrowWidth ($arrowWidth / 2) 0;
      border-color: transparent #000000 transparent transparent;
      border-style: inset;
      width: 0px;
      height: 0px;
      -webkit-transform:rotate(360deg);
   }
   
   .card-subtitle:after {
      text-shadow: 4px 2px 3px rgba(0, 0, 0, 0.0);
      top: 2px;    
   }
   
   
   .card-subtitle .landscape {
      font-size: 11px;
      font-weight: 400;
      padding: 0 21px 0 0;
      text-transform: uppercase;
   }
   
   $smallerLabel : 22px;
   .card-subtitle:after {
      height: $smallerLabel;
      font-size: $smallerLabel;
      right: -(($iconWidth / 2) + 5px);
   }
   .card-subtitle .description,
   .cities .card-subtitle .city-name,
   .cities .card-subtitle .city-price {
      display: none;
   }
   .card-subtitle .landscape {
      display: block;
   }
   .cities .card-title {
      display: block;
   }
   .cities .landscape-price {
      clear: both;
      display: block;
      font-size: 13px;
      font-style: italic;
   }
   .cities .landscape-price b {
      font-size: 17px;
      font-style: normal;
      font-weight: 400;
      padding: 0 0 0 6px;
   }

}

   @media screen and (-webkit-min-device-pixel-ratio:0)
   and (orientation : landscape)  
   {
      .card-subtitle:after {
         top:0;
      }
   }

   
   #book-now {
      background-color:#0bb713 !important;
      margin: 35px 0 0;
      border-top: 1px solid #999;
}

#book-now a {
   display:block;
}
   
#book-now a p {
background: url("images/phone.png") scroll no-repeat left center #0bb713 !important;
padding: 32px 0 35px 80px;
font-size: 29px !important;
font-weight: 100 !important;
margin: 0;
display: inline-block;
}
